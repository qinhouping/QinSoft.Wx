
@{
    ViewBag.Title = "UploadMaterial";
}

<style>
    .bs-form .input-group {
        margin-bottom: 10px;
    }
</style>

<h2>UploadMedia</h2>

<form class="bs-form" method="post" action="@Url.Action("UploadImageMaterialHandle","OfficialAccount")" enctype="multipart/form-data">
    <div class="input-group">
        <span class="input-group-addon" id="basic-addon1">文件</span>
        <input type="file" class="form-control" name="file">
    </div>
    <div class="input-group">
        <input type="submit" class="form-control">
    </div>
</form>